        local req = http_request or request or syn.request
        
        if not req then
            game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "Rogue Hub Error",
                Text = "Your exploit is unsupported with Rogue Hub!",
                Duration = 5
            })
